    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
