{
  "name": "travel-bliss-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "pnpm exec ts-node-dev --respawn src/index.ts",
    "start:prod": "NODE_ENV=production pnpm exec ts-node-dev --respawn src/index.ts",
    "docker:local:up": "docker-compose -f infra/docker-compose-local.yml up",
    "docker:local:down": "docker-compose -f infra/docker-compose-local.yml down",
    "docker:serve:up": "docker-compose -f infra/docker-compose-serve.yml up",
    "docker:serve:down": "docker-compose -f infra/docker-compose-serve.yml down",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm lint:eslint",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "typeorm": "pnpm exec typeorm-ts-node-commonjs -d ./dist/config/data-source.js",
    "migration:generate": "pnpm typeorm migration:generate ./src/migration/migration",
    "migration:up": "pnpm typeorm migration:run",
    "migration:down": "pnpm typeorm migration:revert",
    "test": "pnpm typecheck && pnpm prettier && pnpm lint && pnpm build",
    "test:watch": "pnpm jest --watch --verbose",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "inversify": "^6.2.1",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.10",
    "type-graphql": "2.0.0-beta.3",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^18.19.68",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
